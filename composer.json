{
    "name": "revinate/rabbitmq-bundle",
    "description": "Revinate RabbitMQ bundle which works with Symfony 2",
    "keywords": ["symfony2", "rabbitmq", "message", "queue"],
    "type": "symfony-bundle",
    "license": "MIT",
    "authors": [{
        "name" : "Vinay Kumar"
    }],
    "require": {
        "php"                         : ">=5.3.0",
        "symfony/yaml"                : ">=2.4.8",
        "videlalvaro/php-amqplib"     : "~2.4.0",
        "symfony/dependency-injection": ">=2.4.8",
        "symfony/config": "~2.6"
    },
    "require-dev": {
        "symfony/console": "~2.0"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    },
    "autoload": {
        "psr-0": {
            "Revinate\\RabbitMqBundle\\": ""
        }
    },
    "target-dir" : "Revinate/RabbitMqBundle"
}
